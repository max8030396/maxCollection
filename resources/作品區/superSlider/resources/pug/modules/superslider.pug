mixin superSlider(number)
  .ss_wrap

    .ss_navigator
      .ss_navigator_left
      .ss_navigator_right

    .ss_photo
      - for(var i = 0; i < number; i = i + 1) {
        nav.ss_photo_nav(class=`ss_photo_nav_${i + 1}` data-index=`${i + 1}`)
          a(href="#")
            img(src=`../images/photo${i + 1}.jpg` width="100%" height="100%")
            span!= 'testArea'
      - }

    ul.ss_paginator
      - for(var i = 0; i < number; i = i + 1) {
        li.ss_paginator_doc(class=`ss_paginator_doc_${i + 1}` data-index=`${i + 1}`)
          span
      - }
